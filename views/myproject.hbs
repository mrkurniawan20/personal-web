  {{#>layout}}
  <h1 class="text-center pt-2"><b>ADD MY PROJECT</b></h1>
  <!-- FORM -->
  <form action='' onsubmit='addProject(event)'>
    <div class='form-field'>
      <label for='name'>Project Name</label>
      <input type='text' id='name' required />
    </div>

    <div class='form-date'>
      <div class='start'>
        <label for='start'>Start Date</label>
        <input type='date' id='start' required />
      </div>
      <div class='end'>
        <label for='end'>End Date</label>
        <input type='date' id='end' required />
      </div>
    </div>
    <div class='form-field'>
      <label for='description'>Description</label>
      <textarea name='description' id='description' rows='5'></textarea>
    </div>
    <div class='form-field-tech'>
      <label for='technologies'>Technologies</label>
      <div class='grid-container'>
        <div class='grid'>
          <input type='checkbox' id='nodejs' value='nodejs.png' name='nodejs' />
          <label for='nodejs'>Node JS</label>
        </div>
        <div class='grid'>
          <input type='checkbox' id='express' value='express.png' name='express' />
          <label for='express'>ExpressJS</label>
        </div>
        <div class='grid'>
          <input type='checkbox' id='reacjs' value='reactjs.png' name='reactjs' />
          <label for='reacjs'>React JS</label>
        </div>
        <div class='grid'>
          <input type='checkbox' id='typescript' value='typescript.png' name='typescript' />
          <label for='typescript'>TypeScript</label>
        </div>
      </div>
    </div>
    <div class='form-field'>
      <label for='image'>Upload Image</label>
      <input type='file' id='image' name='image' />
    </div>
    <div class='text-end mb-5'>
      <button class='btn btn-dark' type='submit'>submit</button>
    </div>
  </form>

  <!-- PROJECT LIST -->
  <div class='project-list pb-5'>
    <h1 class="text-center">My Project</h1>
    <section class='project-container' id='project-container'>
